{
  "$schema": "https://turbo.build/schema.json",
  "extends": ["//"],
  "pipeline": {
    "build": {
      "outputs": ["dist/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "lint": {
      "outputs": []
    },
    "test": {
      "outputs": ["coverage/**"]
    },
    "test:ui": {
      "cache": false,
      "persistent": true
    },
    "test:run": {
      "outputs": ["coverage/**"]
    },
    "test:coverage": {
      "outputs": ["coverage/**"]
    },
    "preview": {
      "cache": false,
      "persistent": true
    },
    "type-check": {
      "outputs": []
    },
    "clean": {
      "cache": false
    }
  }
}